   "source": [
    "#Function for getting recommendation\n",
    "def get_recommendation(title,cosine_mat,num_of_rec=10):\n",
    "    #Index of the title\n",
    "    movie_indices = pd.Series(netfx.index, index=netfx['clean_titles']).drop_duplicates()\n",
    "    #Index of course\n",
    "    idx = movie_indices[title]\n",
    "    \n",
    "    #Look into the cosine mat for the index\n",
    "    sim_scores= list(enumerate(cosine_mat[idx]))\n",
    "    sim_scores_sorted = sorted(sim_scores, key=lambda x: x[1],reverse=True)\n",
    "    selected_movies_indices = [i[0] for i in sim_scores_sorted[1:] ]\n",
    "    selected_movies_scores = [i[1] for i in sim_scores_sorted[1:] ]\n",
    "    \n",
    "    #Get the dataframe and the title\n",
    "    result_netfx=netfx.iloc[selected_movies_indices]\n",
    "    result_netfx['similarity_score'] = selected_movies_scores\n",
    "    final_recommended_movies = result_netfx[['title','similarity_score','clean_titles','cast','description','release_year','listed_in','date_added','country','director','type','duration','rating']]\n",
    "    return final_recommended_movies.head(num_of_rec)"
   ]
  },
